{
  "name": "gatsby-starter-mate",
  "version": "2.5.1",
  "description": "Gatsby v2 starter to create a top notch portfolio!",
  "main": "index.js",
  "scripts": {
    "fix-medium-plugin": "sed -i' ' 's/\\/latest//g' ./node_modules/gatsby-source-medium/gatsby-node.js",
    "prepare": "npm run fix-medium-plugin && gatsby clean",
    "build": "npm run prepare && gatsby build",
    "develop": "npm run prepare && gatsby develop",
    "start": "npm run develop",
    "serve": "npm run build && gatsby serve -p 9000",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmaSuriano/gatsby-starter-mate.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Ema Suriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmaSuriano/gatsby-starter-mate/issues"
  },
  "homepage": "https://github.com/EmaSuriano/gatsby-starter-mate#readme",
  "dependencies": {
    "@tippy.js/react": "^3.1.1",
    "contentful": "^9.2.11",
    "dotenv": "^16.0.3",
    "envfile": "^6.18.0",
    "gatsby": "^5.0.1",
    "gatsby-plugin-google-analytics": "^5.0.0",
    "gatsby-plugin-manifest": "^5.0.0",
    "gatsby-plugin-offline": "^6.0.0",
    "gatsby-plugin-preload-link-crossorigin": "^1.0.2",
    "gatsby-plugin-react-helmet": "^6.0.0",
    "gatsby-plugin-styled-components": "^6.0.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-contentful": "^8.0.0",
    "gatsby-source-medium": "^5.0.0",
    "gatsby-transformer-remark": "^6.0.0",
    "inquirer": "^9.1.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.7.1",
    "react-headroom": "^3.2.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.3",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^3.2.5",
    "react-text-loop": "^2.3.0",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "chalk": "^5.1.2",
    "contentful-import": "^8.5.7",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
